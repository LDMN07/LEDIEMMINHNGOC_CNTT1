class bai11{
int main() {
    Fraction f;
    f.input();
    f.output();
    return 0;
}
private:
    int numerator; 
    int denominator; 

public:
    Fraction(int n = 0, int d = 1) {
        numerator = n;
        denominator = d;
    }

    void input() {
        cout << "Enter the numerator: ";
        cin >> numerator;
        cout << "Enter the denominator: ";
        cin >> denominator;
    }

    void output() {
        cout << numerator << "/" << denominator << endl;
    }
}
